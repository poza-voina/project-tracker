{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "RabbitMQ": {
    "Host": "localhost",
    "VirtualHost": "/",
    "Username": "username",
    "Password": "password",
    "ReportResultTaskEndpoint": {
      "Name": "report-result-task"
    },
    "ReportResultTaskGroupEndpoint": {
      "Name": "report-result-group"
    },
    "ReportInputTaskEndpoint": {
      "Name": "report-input-task"
    },
    "ReportInputTaskGroupEndpoint": {
      "Name": "report-input-group"
    }
  },

  "Minio": {
    "AccessKey": "appaccesskey",
    "SecretKey": "appsecretkey",
    "Endpoint": "localhost:9000",
    "ReportBucket": {
      "BucketName": "project-tracker-reports",
      "ContentType": "application/pdf"
    }
  },

  "ProjectTrackerClient": {
    "TasksBasePath": "/api/tasks/informations",
    "TaskGroupsBasePath": "/api/groups/informations",
    "ClientName": "project-tracker-api",
    "DefaultUri": "http://localhost:5231"
  }
